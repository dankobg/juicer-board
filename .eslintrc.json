{
	"env": {
		"browser": true,
		"es2023": true
	},
	"extends": [
		"eslint:recommended",
		"prettier",
		"prettier/@typescript-eslint",
		"plugin:@typescript-eslint/recommended",
		"plugin:lit/recommended"
	],

	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "lit"],
	"rules": {}
}
