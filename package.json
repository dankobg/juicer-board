	"devDependencies": {
		"@semantic-release/git": "^10.0.1",
		"@typescript-eslint/eslint-plugin": "^8.8.0",
		"@typescript-eslint/parser": "^7.16.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
